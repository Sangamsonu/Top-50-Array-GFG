#include<stdio.h>
int count_occ(int arr[], int n, int k){
    int count = 0;
    for(int i=0; i<n; i++){
        if(arr[i] == k){
            count++;
        }
    }
    return count;
}
int main(){
    int arr[] = {4,2,4,7,8,1,1,1,9};
    int n = sizeof(arr) / sizeof(arr[0]);
    int k = 1;
    printf("%d ",count_occ(arr,n,k));
}
