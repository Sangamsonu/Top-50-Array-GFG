#include<stdio.h>
struct pair{
    int max;
    int min;
};
struct pair Getmaxmin(int arr[], int n){
    struct pair minmax;
    int max = arr[0];
    int min = arr[0];
    
    for(int i=0; i<n; i++){
        if(arr[i] > max){
            max = arr[i];
        }
        if(arr[i] < min){
            min = arr[i];
        }
    }
    minmax.max = max;
    minmax.min = min;

    return minmax;
}
int main(){
    int arr[] = {2,7,55,12,356,78};
    int n = sizeof(arr) / sizeof(arr[0]);
    struct pair result = Getmaxmin(arr,n);
    printf("The maximum element is %d",result.max);
    printf("\nThe minimum element is %d",result.min); 
}
// Time complexity = o(n)
